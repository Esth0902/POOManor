@page "/mapJardin"

<div>
    <Map CurrentFloorImage=images/jardin.png 
         CurrentMap="map" 
         Row="10" 
         Column="20" 
         MonstersList="@JardinMonsters" 
         Zones="@JardinZones"
         Exits="@Exits"
         Doors="@JardinDoors"
         PlayerPositionX="1"
         PlayerPositionY="1"
         CoffrePositionX="4"
         CoffrePositionY="3"
         Etage = "12"
         Etang="@Etang"

 
    />
 
</div>

@code {

    private List<Monsters> JardinMonsters = new List<Monsters>
    {
        new Zombie("zombie1", 3, 4),
        new Ghost("ghost1", 1, 3)
    };

    private List<Zone> JardinZones = new List<Zone>
    {
        new Zone(0, 9, 0, 19),
    };

    private List<Exit> Exits = new List<Exit>
    {
        new Exit(19, 4),
        new Exit(9, 0)
    };

    private List<Door> JardinDoors = new List<Door>
    {
    };

    private etang Etang = new etang(9, 4);

}
}